.UCenter-bg {
  background-image: url(data:image/jpeg;base64,);
  background-size: cover;
  height: 400rpx;
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}
.UCenter-bg text {
  opacity: 0.8;
}
.UCenter-bg image {
  width: 200rpx;
  height: 200rpx;
}
.UCenter-bg .gif-wave {
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 1;
  mix-blend-mode: screen;
  height: 100rpx;
}
.block-wrap {
  background-color: #fff;
  padding: 30rpx;
}
.title-wrap {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.title-wrap image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 12rpx;
}
.title {
  font-size: 28rpx;
  color: #333;
}
.foot {
  font-size: 28rpx;
  color: #aa7e56;
  position: relative;
  padding: 0 30rpx;
}
.foot::after {
  content: " ";
  display: inline-block;
  width: 12rpx;
  height: 12rpx;
  position: absolute;
  top: 50%;
  right: 4rpx;
  border-width: 4rpx 4rpx 0 0;
  border-color: #aaa;
  border-style: solid;
  transform: translateY(-50%) matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
}
.moneny-box {
  justify-content: space-around;
}
.moneny-box > view {
  flex: 1;
  display: flex;
  align-items: center;
}
.get-money {
  display: flex;
  align-items: center;
  justify-content: center;
}
.get-money image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 12rpx;
}
.phone {
  position: fixed;
  z-index: 2;
  top: 100rpx;
  left: 10%;
}
.copyright {
  position: absolute;
  bottom: 24rpx;
  left: 50%;
  transform: translate(-50%);
}
button.title-wrap {
  line-height: 1;
  margin: 0;
  background: #fff;
  padding: 0;
}
button::after {
  border: none;
}
input {
  outline: none;
  border: none;
  list-style: none;
}
.flex {
  align-items: center;
}
