.content-box {
  background: #fff;
  padding: 24rpx;
  box-sizing: border-box;
  .title {
    font-weight: 600;
    font-size: 30rpx;
  }
  .time {
    color: #ccc;
    margin-top: 12rpx;
    margin-bottom: 24rpx;
  }
  image {
    width: 100%;
  }
  .rich {
    padding: 24rpx 0;
    box-sizing: border-box;
  }
}
